generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model level_times {
  id                 Int       @id @default(autoincrement())
  player_name        String?
  level_name         String?
  time_elapsed       Float?    @db.Real
  checkpoint_time    Float?    @db.Real
  reached_checkpoint Boolean?
  created_at         DateTime? @default(now()) @db.Timestamptz(6)
}
